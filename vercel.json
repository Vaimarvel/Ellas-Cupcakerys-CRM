{
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" },
    { "src": "dashboard/package.json", "use": "@vercel/static-build", "config": { "buildCommand": "npm run build", "outputDirectory": "dashboard/dist" } }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/index.py" },
    { "src": "/assets/(.*)", "dest": "dashboard/dist/assets/$1" },
    { "src": "/(.*)", "dest": "dashboard/dist/index.html" }
  ]
}
